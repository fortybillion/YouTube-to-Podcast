{
	"filesystem": {
		"path": "/path/to/output",
		"deleteOld": true,
		"baseUrl": "http://your.domain/podcast/"
	},
	"log": {
		"console": {
			"enabled": true,
			"level": "debug",
			"timestamp": true
		},
		"local": {
			"enabled": false,
			"level": "info",
			"filename": "service.log"
		},
		"cloudwatch": {
			"enabled": false,
			"level": "info",
			"logGroupName": "youtube-to-podcast",
			"jsonMessage": true
		}
	},
	"podcast": {
		"xml": "feed.xml",
		"feed": {
			"title": "PODCAST TITLE",
			"description": "PODCAST DESCRIPTION",
			"site_url": "SITE URL",
			"feed_url": "PUBLIC FEED XML",
			"image_url": "LOGO URL",
			"author": "PODCAST AUTHOR",
			"language": "en",
			"categories": [
				"CATEGORY"
			],
			"ttl": "3600",
			"itunesAuthor": "PODCAST AUTHOR",
			"itunesSubtitle": "PODCAST SUBTITLE",
			"itunesSummary": "PODCAST SUMMARY",
			"itunesOwner": {
				"name": "PODCAST OWNER",
				"email": "OWNER EMAIL"
			},
			"itunesExplicit": false,
			"itunesCategory": [
				{
					"text": "CATEGORY"
				}
			],
			"itunesImage": "LOGO URL"
		},
		"item": {
			"categories": [
				"CATEGORY"
			],
			"itunesAuthor": "PODCAST AUTHOR",
			"itunesKeywords": [
				"SOME",
				"KEYWORDS"
			]
		}
	},
	"youtube": {
		"channel": "CHANNEL_ID",
		"maxEpisodes": 5,
		"regex": null,
		"order": "date",
		"minDuration": 600,
		"promiseLimit": 3,
		"fixSeekTable": true
	}
}